# Find package module for COLMAP library.
#
# The following variables are set by this module:
#
#   @LW_EXPORT_PROJECT_NAME@_FOUND: TRUE if COLMAP is found.
#   @LW_EXPORT_PROJECT_NAME@_VERSION: COLMAP version.
#   @LW_EXPORT_PROJECT_NAME@_INCLUDE_DIRS: Include directories for COLMAP.
#   @LW_EXPORT_PROJECT_NAME@_LINK_DIRS: Link directories for COLMAP.
#   @LW_EXPORT_PROJECT_NAME@_LIBRARIES: Libraries required to link COLMAP.

get_filename_component(@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX ${CMAKE_CURRENT_LIST_FILE} PATH)
set(@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX "${@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX}/../..")
set(@LW_EXPORT_PROJECT_NAME@_FOUND FALSE)

# Find dependency packages.
set(TEMP_CMAKE_MODULE_PATH ${CMAKE_MODULE_PATH})
set(CMAKE_MODULE_PATH ${@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX}/share/@PROJECT_NAME@/cmake_modules)

# Set the exported variables.
set(@LW_EXPORT_PROJECT_NAME@_FOUND TRUE)

set(@LW_EXPORT_PROJECT_NAME@_VERSION @TICPP_VERSION@)

set(@LW_EXPORT_PROJECT_NAME@_INCLUDE_DIRS
    ${@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX}/include/
    # ${@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX}/include/@PROJECT_NAME@
)

set(@LW_EXPORT_PROJECT_NAME@_LINK_DIRS
    ${@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX}/libs/${CMAKE_SYSTEM_PROCESSOR}
)

set(@LW_EXPORT_PROJECT_NAME@_INTERNAL_LIBRARIES
    ""
)

set(@LW_EXPORT_PROJECT_NAME@_EXTERNAL_LIBRARIES
    ""
)

set(@LW_EXPORT_PROJECT_NAME@_LIBRARIES
    @PROJECT_NAME@_lib
    ${@LW_EXPORT_PROJECT_NAME@_INTERNAL_LIBRARIES}
    ${@LW_EXPORT_PROJECT_NAME@_EXTERNAL_LIBRARIES}
)

# Cleanup of configuration variables.
set(CMAKE_MODULE_PATH ${TEMP_CMAKE_MODULE_PATH})

unset(@LW_EXPORT_PROJECT_NAME@_INSTALL_PREFIX)
